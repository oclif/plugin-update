    "@types/execa": "^0.9.0",
    "@types/fs-extra": "^8.0.1",
    "@types/glob": "^7.1.3",
    "@types/inquirer": "^8.2.0",
    "@types/lodash.throttle": "^4.1.6",
    "@types/mocha": "^9",
