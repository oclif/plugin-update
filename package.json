{
  "name": "@oclif/plugin-update",
  "version": "4.6.35",
  "author": "Salesforce",
  "bugs": "https://github.com/oclif/plugin-update/issues",
  "dependencies": {
    "@inquirer/select": "^2.5.0",
    "@oclif/core": "^4",
    "@oclif/table": "^0.4.6",
    "ansis": "^3.17.0",
    "debug": "^4.4.0",
    "filesize": "^6.1.0",
    "got": "^13",
    "proxy-agent": "^6.5.0",
    "semver": "^7.6.3",
    "tar-fs": "^2.1.2"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^19",
    "@eslint/compat": "^1.2.7",
    "@oclif/plugin-help": "^6",
    "@oclif/prettier-config": "^0.2.1",
    "@types/chai": "^4.3.17",
    "@types/debug": "^4.1.12",
    "@types/execa": "^0.9.0",
    "@types/mocha": "^10",
    "@types/node": "^18",
    "@types/semver": "^7.5.8",
    "@types/sinon": "^17.0.3",
    "@types/tar-fs": "^2.0.2",
    "chai": "^4.5.0",
    "commitlint": "^19",
    "eslint": "^9.21.0",
    "eslint-config-oclif": "^6.0.20",
    "eslint-config-prettier": "^10.1.1",
    "husky": "^9.1.7",
    "lint-staged": "^15",
    "mocha": "^10.8.2",
    "nock": "^13.5.6",
    "oclif": "^4",
    "prettier": "^3.5.3",
    "shx": "^0.3.4",
    "sinon": "^18.0.1",
    "strip-ansi": "^7.1.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.3"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "oclif.manifest.json",
    "/dist"
  ],
  "homepage": "https://github.com/oclif/plugin-update",
  "keywords": [
    "oclif-plugin"
  ],
  "license": "MIT",
  "oclif": {
    "autoupdate": "github",
    "commands": "./dist/commands",
    "bin": "oclif-example",
    "hooks": {
      "init": "./dist/hooks/init.js"
    },
    "devPlugins": [
      "@oclif/plugin-help"
    ],
    "flexibleTaxonomy": true
  },
  "repository": "oclif/plugin-update",
  "scripts": {
    "build": "shx rm -rf lib && tsc",
    "clean": "shx rm -f oclif.manifest.json",
    "compile": "tsc",
    "lint": "eslint",
    "postpack": "yarn run clean",
    "posttest": "yarn lint",
    "prepack": "yarn build && oclif manifest && oclif readme",
    "prepare": "husky && yarn build",
    "pretest": "yarn build --noEmit && tsc -p test --noEmit",
    "test:integration:sf": "mocha --forbid-only \"test/integration/sf.integration.ts\" --timeout 900000",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md"
  },
  "exports": "./dist/index.js",
  "type": "module"
}
